<UserControl x:Class="WPFContactControls.PhoneNumberPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="104" Width="364">
    <Grid>
        <TextBox Margin="26,0,0,25" Name="txtAreaCode" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="33" />
        <Label Margin="12,51,0,21" Name="lblLeftParen" HorizontalAlignment="Left" Width="17" FontSize="17">(</Label>
        <Label FontSize="17" HorizontalAlignment="Left" Margin="57,51,0,21" Name="lblRightParen" Width="17">)</Label>
        <TextBox Height="23" Margin="80,0,0,25" Name="txtPrefix" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="35" />
        <Label HorizontalAlignment="Left" Margin="119,52,0,24" Name="lblDash" Width="27" FontSize="17">--</Label>
        <TextBox Height="23" Margin="144,0,0,26" Name="txtSuffix" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="45" />
        <Label Margin="195,0,0,23" Name="lblExtension" Height="28" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="31">Ext.</Label>
        <TextBox Margin="232,0,0,28" Name="txtExtension" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="120" />
        <ComboBox Height="23" Margin="12,12,12,0" Name="cmbType" VerticalAlignment="Top" />
    </Grid>
</UserControl>
