<UserControl x:Class="WPFContactControls.AddressPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="172" Width="300">
    <Grid>
        <Label HorizontalAlignment="Left" Margin="12,12,0,0" Name="lblStreet" Width="46" Height="28" VerticalAlignment="Top">Street</Label>
        <TextBox Margin="64,12,12,137" Name="txtStreet" />
        <Label Margin="12,0,0,105" Name="lblCity" HorizontalAlignment="Left" Width="46" Height="28" VerticalAlignment="Bottom">City</Label>
        <TextBox Height="23" Margin="64,0,12,108" Name="txtCity" VerticalAlignment="Bottom" />
        <Label Height="28" Margin="12,0,0,71" Name="lblState" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="46">State</Label>
        <TextBox Height="23" Margin="64,0,12,79" Name="txtState" VerticalAlignment="Bottom" />
        <TextBox Height="23" Margin="64,0,12,50" Name="txtZip" VerticalAlignment="Bottom" />
        <Label Height="28" Margin="12,0,0,47" Name="lblZip" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="30">Zip</Label>
        <ComboBox Height="19.277" Margin="64,0,12,24" Name="cmbCountry" VerticalAlignment="Bottom" VerticalContentAlignment="Center" />
        <Label HorizontalAlignment="Left" Margin="9,0,0,13" Name="lblCountry" Width="49" Height="28" VerticalAlignment="Bottom">Country</Label>
    </Grid>
</UserControl>
